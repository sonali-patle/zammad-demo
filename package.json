{
  "private": true,
  "scripts": {
    "generate-graphql-api": "RAILS_ENV=development bundle exec rails generate graphql_introspection > tmp/graphql_introspection.json && npx graphql-codegen -c .graphql_code_generator.yml && rm tmp/graphql_introspection.json",
    "dev": "RAILS_ENV=development forego start -f Procfile.dev",
    "i18n": "rails generate translation_catalog",
    "lint": "vue-tsc --noEmit && eslint --cache --cache-location ./tmp/eslintcache.js -c .eslintrc.js --ext .js,.ts,.vue app/frontend/ .eslint-plugin-zammad/ .storybook/",
    "lint:fix": "yarn lint -- --fix",
    "lint:css": "stylelint **/*.{css,vue,scss}",
    "lint:css:fix": "stylelint **/*.{css,vue,scss} --fix",
    "test": "VTL_SKIP_AUTO_CLEANUP=true TZ=utc vitest",
    "test:ct": "yarn --cwd ./.cypress cypress open --component --project ../ --config-file .cypress/cypress.config.ts",
    "test:ci:ct": "yarn --cwd ./.cypress cypress run --component --project ../ --config-file .cypress/cypress.config.ts",
    "cypress:install": "yarn --cwd ./.cypress install",
    "postinstall": "node script/fix-node-modules.mjs",
    "storybook": "yarn --cwd ./.storybook start",
    "storybook:build": "yarn --cwd ./.storybook build",
    "storybook:install": "yarn --cwd ./.storybook install"
  },
